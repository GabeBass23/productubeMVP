{
    "manifest_version": 3,
    "name": "ProducTube",
    "description": "Limits distractions on YouTubeâ„¢",
    "version": "1.0",
    "background": {
      "service_worker": "serviceWorker.js"
    },
    "permissions": [
        "scripting",
        "activeTab",
        "storage"
      ],
    "web_accessible_resources": [{
      "resources": ["html/homepageStruct.html"],
      "matches": ["*://*.youtube.com/*"]
    }],
    "host_permissions": [
        "*://*.youtube.com/*"
    ]
  }